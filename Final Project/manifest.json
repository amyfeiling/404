{
  "name": "Changes the background and foreground color",
  "description": "Change the current page formatting colors",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
      "default_title": "Set browser's default colors.",
      "default_icon": "colorpicker.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}